{
  "name": "tblv3-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "test-ci": "jest --coverage --ci --passWithNoTests --bail",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test-watch": "jest --watchAll=true --coverage",
    "commit": "git-cz",
    "prepare": "husky install",
    "c": "yarn commit --no-verify",
    "amend": "git commit --amend --no-verify --no-edit",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toursbylocals/tblv3-api.git"
  },
  "keywords": [],
  "author": "ToursByLocals",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toursbylocals/tblv3-api/issues"
  },
  "homepage": "https://github.com/toursbylocals/tblv3-api#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "2.7.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.6.0",
    "@graphql-codegen/typescript-resolvers": "2.6.7",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "dotenv": "10.0.0",
    "eslint": "^8.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog-emoji"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write",
      "yarn test-ci"
    ]
  },
  "dependencies": {
    "@types/validator": "^13.7.4",
    "apollo-server-express": "^3.9.0",
    "express": "4.17.1",
    "graphql": "^16.5.0",
    "graphql-middleware": "^6.1.31",
    "graphql-shield": "^7.5.0",
    "luxon": "^2.4.0",
    "mongoose": "^6.4.3",
    "validator": "^13.7.0"
  }
}
